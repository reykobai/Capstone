@model IEnumerable<ProjectCollaborationSystem.Models.Project>

@{
    ViewBag.Title = "DelayedProject";
}

<div class="panel panel-orange">
    <div class="panel-heading">Pending Project(s)</div>
    <div class="panel-body">
        <table class="table table-hover">
            <thead>
                <tr>

                    <th>Title</th>
                    <th>Date Added</th>
                    <th>Due Date</th>
                    <th>Status</th>
                    <th>Priority</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>

                        <td> @item.ProjectTitle </td>
                        <td> @item.DateAdded </td>
                        <td> @item.DueDate </td>
                        <td> @item.ProjectStatus </td>
                        <td> @item.ProjectPriority </td>
                        <td>@Html.ActionLink("Open", "DisplayProjet", "Project", new { id = @item.ProjectID }, null)</td>



                    </tr>


                }

            </tbody>
        </table>

    </div>
</div>



